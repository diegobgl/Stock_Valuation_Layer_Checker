<odoo>
    <record id="view_check_valuation_layer_wizard" model="ir.ui.view">
        <field name="name">check.valuation.layer.wizard.form</field>
        <field name="model">check.valuation.layer.wizard</field>
        <field name="arch" type="xml">
            <form string="Verificar Stock Valuation Layers Sin Asientos">
                <group>
                    <field name="start_date" required="1"/>
                    <field name="end_date" required="1"/>
                </group>
                <footer>
                    <button string="Buscar" type="object" name="search_valuation_layers_without_accounting" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
